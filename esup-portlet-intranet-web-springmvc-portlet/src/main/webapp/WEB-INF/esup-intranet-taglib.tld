<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
    
    <description>JSTL 1.1 functions library</description>
    <display-name>JSTL functions</display-name>
    <tlib-version>1.1</tlib-version>
    <short-name>esup</short-name>
    <uri>http://esup.org/jsp/jstl/nuxeo/functions</uri>
    
    <function>
        <description> Get the value of PropertyMap using key </description>
        <name>getValue</name>
        <function-class>org.esup.portlet.intranet.web.taglib.IntranetTagLib</function-class>
        <function-signature>String getValue(org.nuxeo.ecm.automation.client.model.PropertyMap, java.lang.String)</function-signature>
        <example>
            ${esup:getValue(obj, 'java.lang.String')}
        </example>
    </function>
    
    <function>
        <description> Get the last modified date from PropertyMap </description>
        <name>getLastModifiedDate</name>
        <function-class>org.esup.portlet.intranet.web.taglib.IntranetTagLib</function-class>
        <function-signature>String getLastModifiedDate(org.nuxeo.ecm.automation.client.model.PropertyMap)</function-signature>
        <example>
            ${esup:getLastModifiedDate(obj)}
        </example>
    </function>
    
    <function>
        <description> Get img file name </description>
        <name>getImgFileName</name>
        <function-class>org.esup.portlet.intranet.web.taglib.IntranetTagLib</function-class>
        <function-signature>String getImgFileName(org.nuxeo.ecm.automation.client.model.PropertyMap)</function-signature>
        <example>
            ${getImgFileName('org.nuxeo.ecm.automation.client.model.PropertyMap')}
        </example>
    </function>
    
    <function>
        <description> checks existance of the file </description>
        <name>hasFicher</name>
        <function-class>org.esup.portlet.intranet.web.taglib.IntranetTagLib</function-class>
        <function-signature>boolean hasFicher(org.nuxeo.ecm.automation.client.model.PropertyMap)</function-signature>
        <example>
            ${hasFicher('org.nuxeo.ecm.automation.client.model.PropertyMap')}
        </example>
    </function>
    
     <function>
        <description> checks whether a file or directory </description>
        <name>isFolder</name>
        <function-class>org.esup.portlet.intranet.web.taglib.IntranetTagLib</function-class>
        <function-signature>boolean isFolder(org.nuxeo.ecm.automation.client.model.Document)</function-signature>
        <example>
            ${isFolder('org.nuxeo.ecm.automation.client.model.Document')}
        </example>
    </function>   
    
    
</taglib>